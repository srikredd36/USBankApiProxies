<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<KeyValueMapOperations name="ReadKVMs" mapIdentifier="ApigeeManagementServiceProxy-v1" enabled="true">
    <Scope>environment</Scope>
    <ExclusiveCache>false</ExclusiveCache>
    <ExpiryTimeInSecs>-1</ExpiryTimeInSecs>
    <Get assignTo="ManagementServicePath_KVM">
        <Key>
            <Parameter>ManagementServicePath</Parameter>
        </Key>
    </Get>
    <Get assignTo="QueryString.name">
        <Key>
            <Parameter>CustomAttributeName</Parameter>
        </Key>
    </Get>
    <Get assignTo="QueryString.brandSourceValue">
        <Key>
            <Parameter>CustomAttributeValues</Parameter>
        </Key>
    </Get>
</KeyValueMapOperations>