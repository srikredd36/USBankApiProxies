<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<MessageLogging name="LogProxyResponse" async="true" continueOnError="true" enabled="true">
    <File>
        <Message>

system.time={system.time} 
system.timestamp={system.timestamp}
client.sent.end.timestamp={client.sent.end.timestamp}
client.sent.start.timestamp={client.sent.start.timestamp}
message.reason.phrase={message.reason.phrase}
response.status.code={response.status.code}
=====
response.content={response.content}
=====
</Message>
        <FileName>ProxyResponse.log</FileName>
        <FileRotationOptions rotateFileOnStartup="true">
            <FileRotationType>SIZE</FileRotationType>
            <MaxFileSizeInMB>10</MaxFileSizeInMB>
            <MaxFilesToRetain>10</MaxFilesToRetain>
        </FileRotationOptions>
    </File>
    <logLevel>INFO</logLevel>
</MessageLogging>